    "editor.fontFamily": " 'JetBrainsMono Nerd Font', 'Lekton Nerd Font Mono'",
    "editor.linkedEditing": true,
    "editor.defaultColorDecorators": true,
    "editor.minimap.scale": 1,
    "editor.lineHeight": 23,
    "editor.fontSize": 13,
    "editor.fontWeight": "100",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 15,
    "editor.occurrencesHighlight": "singleFile",
    "editor.cursorBlinking": "smooth",
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.bracketPairColorization.enabled": false,
    "editor.detectIndentation": false,
    "editor.guides.bracketPairs": true,
    "editor.suggestOnTriggerCharacters": true,
    "editor.suggest.localityBonus": true,
    "editor.fontLigatures": true,
    "editor.autoClosingQuotes": "languageDefined",
    "editor.suggestSelection": "first",
    // "editor.lineNumbers": "relative",
    "editor.tokenColorCustomizations": {
        "comments": "#5f7191",
        // "comments": "#455f8d",

        "[Blueberry dark theme]": {
            "textMateRules": [
                {
                    "scope": "string",
                    "settings": {
                        // "foreground": "#b2d3c7"
                        // "foreground": "#b2d3c7"
                        "foreground": "#8dc3c5"
                    }
                }
            ]
        },

        "[Chogori Darkest Blue]": {
            "textMateRules": [
                {
                    "scope": "string",
                    "settings": {
                        "foreground": "#50caec"
                    }
                },
                {
                    "scope": [
                        "keyword.control.export.js",
                        "keyword.control.import.js"
                    ],
                    "settings": {
                        "foreground": "#d19515",
                        "fontStyle": "bold"
                    }
                }
            ]
        },

        "[React Theme]": {
            "textMateRules": [
                {
                    "scope": "string",
                    "settings": {
                        "foreground": "#8fac89"
                    }
                },
                {
                    "scope": [
                        "comment.line.double-slash.js",
                        "comment.block.documentation.js",
                        "punctuation.definition.comment.js",
                        "comment.line.number-sign.python"
                    ],
                    "settings": {
                        "foreground": "#647580"
                        // "fontStyle": ""
                    }
                },
                {
                    "scope": "comment.block.documentation.js",
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": ["keyword", "storage.type.js"],
                    "settings": {
                        "foreground": "#d8b5e4"
                    }
                },
                {
                    "scope": ["storage.type.class.jsdoc"],
                    "settings": {
                        // "foreground": "#00b4b7"
                        "foreground": "#9d9d9d"
                        // "foreground": "#df7b00"
                    }
                },
                {
                    "scope": ["variable.other.jsdoc"],
                    "settings": {
                        // "foreground": "#00b4b7"
                        "foreground": "#9d9d9d"
                        // "foreground": "#df7b00"
                    }
                }
            ]
        }
    },
    "terminal.integrated.fontFamily": "Lekton Nerd Font Mono",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.tabs.enabled": false,
    "terminal.integrated.inheritEnv": false,
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "files.associations": {
        "*tmpl": "html"
    },
    "[json]": {
        "editor.tabSize": 2,
        "editor.suggest.insertMode": "replace"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 4
    },
    "[css]": {
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 4
    },
    "[javascript]": {
        "editor.maxTokenizationLineLength": 2500,
        "prettier.tabWidth": 4
    },
    "[sql]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "[python]": {
        "editor.tabSize": 4,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit",
            "source.fixAll": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "gitlens.codeLens.symbolScopes": ["!Module"],
        "editor.wordBasedSuggestions": "off"
    },
    // "[go]": {
    //   "editor.defaultFormatter": "golang.go",
    //   "editor.tabSize": 4,
    //   "editor.insertSpaces": false,
    //   "editor.formatOnSave": true,
    //   "editor.codeActionsOnSave": {
    //     "source.organizeImports": "always"
    //   }
    // },
    // "[c][cpp]": {
    //   "editor.tabSize": 4,
    //   "editor.defaultFormatter": "ms-vscode.cpptools",
    //   "editor.formatOnSave": true,
    //   "editor.wordBasedSuggestions": "currentDocument",
    //   "editor.suggest.insertMode": "replace",
    //   "editor.semanticHighlighting.enabled": true
    // },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "prettier.jsxSingleQuote": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "files.autoSaveDelay": 10000,
    "files.autoSave": "off",
    "extensions.ignoreRecommendations": true,
    "eslint.run": "onSave",
    "scm.diffDecorations": "none",
    "sqltools.format": {
        "language": "sql",
        "linesBetweenQueries": 2,
        "reservedWordCase": "upper"
    },
    "sqltools.useNodeRuntime": true,
    "cSpell.language": "en-GB",
    "cSpell.diagnosticLevel": "Hint",
    "files.exclude": {
        "**/__pycache__": true
    },
    "workbench.iconTheme": "seti"
}
