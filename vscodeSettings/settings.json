{
  "security.workspace.trust.untrustedFiles": "open",
  // ***************************
  // ** globals and basics
  // ***************************
  "window.zoomLevel": -0.5,
  "window.titleBarStyle": "custom",
  "explorer.compactFolders": false,
  "debug.allowBreakpointsEverywhere": true,
  "editor.minimap.scale": 1,
  "gitlens.showWelcomeOnInstall": false,
  "material-icon-theme.showWelcomeMessage": false,
  "terminal.integrated.inheritEnv": false,
  "files.trimTrailingWhitespace": true,
  "explorer.confirmDragAndDrop": false,
  // ***************************
  // ** workbench
  // ***************************
  "workbench.tree.indent": 32,
  "workbench.editor.wrapTabs": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.editorAssociations": {
    "*.db": "sqlite-viewer.option"
  },
  "workbench.colorCustomizations": {
    // *************************
    // ** Current default light
    // ** theme coice
    // *************************
    "[Ease]": {
      "titleBar.activeBackground": "#C9C8BA",
      "sideBarSectionHeader.border": "#D9D8D1",
      "sideBarSectionHeader.background": "#E3E2DB",
      "editorLineNumber.foreground": "#999888",
      "statusBar.background": "#3e425bca",
      "panel.background": "#D9D8D0",
      "terminal.background": "#D9D8D0",
      "tab.activeBackground": "#D1D0C2",
      "tab.activeBorder": "#6673e6",
      "tab.border": "#bcbcb7",
      "editor.lineHighlightBackground": "#DAD9D1",
      "terminalCursor.foreground": "#403c3c",
      "terminalCursor.background": "#e4e0e0",
      "editorSuggestWidget.selectedBackground": "#81a5e3bb",
      "editorSuggestWidget.background": "#d0cfc1",
      // separators
      "separators.interfaces.borderColor": "#C9C8BA",
      "separators.methods.borderColor": "#C9C8BA",
      "separators.functions.borderColor": "#C9C8BA",
      "separators.constructors.borderColor": "#C9C8BA",
      "separators.namespaces.borderColor": "#C9C8BA",
      "separators.enums.borderColor": "#C9C8BA",
      "separators.structs.borderColor": "#C9C8BA",
      "separators.classes.borderColor": "#C9C8BA"
    },
    // *************************
    // ** Current default light
    // ** theme coice
    // *************************
    "[Gatito Theme]": {
      "editorSuggestWidget.selectedBackground": "#8f3964",
      "activityBar.background": "#161c1e",
      "activityBar.border": "#141a1c",
      "sideBarSectionHeader.background": "#202728",
      "sideBarSectionHeader.border": "#2c3234",
      "editorLineNumber.activeForeground": "#00ddff",
      "editor.foreground": "#b3b4bd",
      "tab.activeBackground": "#2f3638",
      "tab.activeForeground": "#7bedff",
      "tab.inactiveForeground": "#666666",
      "tab.activeBorderTop": "#51d8ed",
      "terminal.foreground": "#8a90a0",
      // separators
      "separators.interfaces.borderColor": "#373737",
      "separators.methods.borderColor": "#373737",
      "separators.functions.borderColor": "#373737",
      "separators.constructors.borderColor": "#373737",
      "separators.namespaces.borderColor": "#373737",
      "separators.enums.borderColor": "#373737",
      "separators.structs.borderColor": "#373737",
      "separators.classes.borderColor": "#373737"
    },
    "[Default Dark+][Visual Studio Dark]": {
      // app container
      "titleBar.activeBackground": "#252525",
      "activityBar.background": "#272727",
      "activityBar.border": "#2d2d2d",
      //"activityBar.inactiveForeground": "#547a90",
      "scrollbar.shadow": "#232323",
      "statusBar.border": "#212121",
      "statusBar.background": "#323232",
      "sideBar.background": "#252525",
      "sideBar.border": "#373737",
      "sideBarSectionHeader.border": "#3c3b3b",
      "sideBar.foreground": "#9d9dad",
      "gitDecoration.ignoredResourceForeground": "#73737f",
      "activityBar.foreground": "#e3d4ab",
      "activityBar.inactiveForeground": "#5e5e54",
      "activityBarBadge.background": "#e0a505",
      "activityBarBadge.foreground": "#393937",
      // terminal
      "terminalCursor.foreground": "#eb4e7d",
      // editor
      "editor.background": "#232323",
      "editorLineNumber.foreground": "#555",
      "tab.activeBackground": "#555",
      "tab.activeBorderTop": "#42a1d4",
      "tab.activeBorder": "#6ac3ed",
      "editorCursor.foreground": "#fbc11f",
      "editor.lineHighlightBorder": "#262626",
      "editor.lineHighlightBackground": "#262626",
      // separators
      "separators.interfaces.borderColor": "#373737",
      "separators.methods.borderColor": "#373737",
      "separators.functions.borderColor": "#373737",
      "separators.constructors.borderColor": "#373737",
      "separators.namespaces.borderColor": "#373737",
      "separators.enums.borderColor": "#373737",
      "separators.structs.borderColor": "#373737",
      "separators.classes.borderColor": "#373737"
    }
  },
  // ***************************
  // ** editor general settings
  // ***************************
  //"editor.fontFamily": " 'Lekton Nerd Font Mono', 'InconsolataGo Nerd Font Mono', 'Operator Mono Lig', Menlo, Monaco",
  //"editor.fontFamily": " 'Inconsolata Nerd Font Mono', 'Operator Mono Lig', Menlo, Monaco",
  "editor.fontFamily": "'Operator Mono Lig', 'Lekton Nerd Font Mono', 'InconsolataGo Nerd Font Mono', Menlo, Monaco",
  //"editor.fontFamily": "'MesloLGS NF'",
  //"editor.fontFamily": "'Monofur Nerd Font'",
  //"editor.fontFamily": "'UbuntuMono Nerd Font'",
  //"editor.fontFamily": "'Hack Nerd Font'",
  //"editor.fontFamily": "'Comic Mono'",

  "editor.tabSize": 2,
  "editor.lineHeight": 22,
  "editor.fontSize": 13.85,
  "editor.fontWeight": "100",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 27,
  "editor.cursorBlinking": "smooth",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.linkedEditing": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.detectIndentation": false,
  "editor.guides.bracketPairs": true,
  "editor.suggestOnTriggerCharacters": true,
  "editor.suggest.localityBonus": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.wordBasedSuggestions": true,
  "editor.parameterHints.enabled": true,
  "editor.tokenColorCustomizations": {
    // **************************
    // ** universal overrides
    // **************************

    "[Default Dark+]": {
      "textMateRules": [
        {
          "scope": "comment, punctuation.definition.comment.js",
          "settings": {
            //"foreground": "#4f5467"
            "foreground": "#4c5e53",
            "fontStyle": "italic"
          }
        },
        {
          "scope": "entity.name.function",
          "settings": {
            //"foreground": "#b3b38c",
            //"foreground": "#afa96f"
            "foreground": "#77aeb4"
          }
        },
        {
          "scope": "string.quoted, string.template",
          "settings": {
            "foreground": "#adbb94"
            //"foreground": "#e88686"
          }
        },
        {
          "scope": "punctuation, meta.brace",
          "settings": {
            "foreground": "#7d9eae"
          }
        }
      ]
    }
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
  "javascript.format.semicolons": "insert",
  // *****************************
  // ** terminal
  // *****************************
  //"terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontFamily": "Lekton Nerd Font Mono",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.tabs.enabled": false,
  // *****************************
  // ** Settings for html emmet css
  // *****************************
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "files.associations": {
    "*tmpl": "html"
  },
  // ******************************
  // ** javascript language support
  // ******************************
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.quoteStyle": "single",
  "[javascript]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.suggestOnTriggerCharacters": true,
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "separators.enabledSymbols": ["Classes", "Functions", "Methods"],
    "separators.maxDepth": 1,
    "editor.language.colorizedBracketPairs": [
      ["[", "]"],
      ["(", ")"]
    ]
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"]
  },

  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.fontLigatures": true,
  "editor.autoClosingQuotes": "always",
  "window.title": "${activeEditorMedium}",
  "window.titleSeparator": "  â€”  ",
  "svelte.enable-ts-plugin": true,
  "reactSnippets.settings.importReactOnTop": false,
  "workbench.colorTheme": "Gatito Theme"
  //"php.validate.executablePath": ""
}
